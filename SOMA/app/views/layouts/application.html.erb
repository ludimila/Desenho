<!DOCTYPE html>
<html>
<head>
  <title>SOMA</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |key, value| %>
    <div class="alert <%= bootstrap_class_for(key) %>" role="alert"><%= value %></div>
  <% end %>

  <%= yield %>
  
  <%= debug(params) if Rails.env.development? %>
</body>
</html>
